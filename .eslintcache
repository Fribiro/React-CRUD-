          this.setState({userdetails: res.data[0]});\n            $(\"#myModal\").modal(\"show\");\n\n          \n          }\n          );\n      }\n      componentDidMount(){\n\n        //Get all users details in bootstrap table\n        axios.get('http://localhost/save.php').then(res => \n        {\n        //Storing users detail in state array object\n        this.setState({data: res.data});\n           }); \n        \n        }\n    \n \n  render() {\n   \n    return (\n     \n      <div className=\"maincontainer\">\n        \n        <h1 className=\"mr-5 ml-5 mt-5\">Reactjs simple crud tutorial for beginners</h1>\n        <div className=\"container mb-5 mt-5 text-left\">\n        <button className=\"bg-primary mb-3\"><Link class=\"nav-link\" to={'/adduser'}><span>Add</span><i class=\"fas fa-user\"></i></Link></button>\n        <table class=\"table table-hover table-bordered\">\n          <thead>\n            <tr>\n              <th>ID</th>\n              <th>Email</th>\n              <th>Username</th>\n              <th>Actions</th>\n            </tr>\n          </thead>\n          <tbody>\n          {this.state.data.map((result) => {\n            return (\n             \n                 <tr>\n                  <td>{result.id}</td>\n                  <td>{result.email}</td>\n                  <td>{result.username}</td>\n                  <td>\n                    <button className=\"bg-info\" onClick={e => {this.userdetails(result.id)}}> <i class=\"fas fa-eye\"></i> </button>\n                    <button className=\"bg-warning\"> <i class=\"fas fa-edit\"></i> </button>\n                    <button className=\"bg-danger\"> <i class=\"fas fa-trash\"></i> </button>\n                  </td>\n                </tr>\n             \n            )\n          })}\n           \n            \n          </tbody>\n        </table>\n\n        \n            \n      </div>\n      <div class=\"modal\" id=\"myModal\">\n            <div class=\"modal-dialog\">\n              <div class=\"modal-content\">\n              \n                <div class=\"modal-header\">\n                  <h4 class=\"modal-title align-center\">User : {this.state.userdetails.username}</h4>\n                  <button type=\"button\" class=\"close\" data-dismiss=\"modal\">&times;</button>\n                </div>\n                \n                <div class=\"modal-body text-center\">\n                <table class=\"table table-hover table-bordered\">\n          <thead>\n            <tr>\n              <th>ID</th>\n              <th>Email</th>\n              <th>Username</th>\n              \n            </tr>\n          </thead>\n          <tbody>\n          \n             \n                  <tr>\n                  <td>{this.state.userdetails.id}</td>\n                  <td>{this.state.userdetails.email}</td>\n                  <td>{this.state.userdetails.username}</td>\n                  </tr>\n           \n            \n          </tbody>\n        </table>\n                </div>\n              \n                <div class=\"modal-footer\">\n                  <button type=\"button\" class=\"btn btn-danger\" data-dismiss=\"modal\">Close</button>\n                </div>\n              </div>\n            </div>\n          </div>\n    \n      </div>\n      \n)\n};\n}\n\nexport default ViewUser;","/opt/lampp/htdocs/reactcrud/src/App.js",["22","23"],{"ruleId":"24","severity":1,"message":"25","line":5,"column":26,"nodeType":"26","messageId":"27","endLine":5,"endColumn":32},{"ruleId":"24","severity":1,"message":"28","line":5,"column":34,"nodeType":"26","messageId":"27","endLine":5,"endColumn":40},{"ruleId":"24","severity":1,"message":"29","line":5,"column":42,"nodeType":"26","messageId":"27","endLine":5,"endColumn":47},{"ruleId":"24","severity":1,"message":"25","line":5,"column":26,"nodeType":"26","messageId":"27","endLine":5,"endColumn":32},{"ruleId":"24","severity":1,"message":"28","line":5,"column":34,"nodeType":"26","messageId":"27","endLine":5,"endColumn":40},{"